/*  Write a function that prints a table of values for sine and cosine between (0, 1)
    -  Coursera - 07/22/2020
*/
#include <stdio.h>
#include <math.h> /* math library */
int main(void)
{
    double x;
/* declaring variable */
 for(x = 0.0; x <1; x += 0.1) /* selecting interval */
{
 printf("sin( %f ) = %f and cos( %f ) = %f \n", x, sin(x),x, cos(x));
/* calculating and printing the answers */
}
   return 0;
}
